{
	"name": "Dark Theme",
	"type": "dark",
	"colors": {
		// Activity Bar - https://code.visualstudio.com/docs/getstarted/theme-color-reference#_activity-bar
		"activityBar.background": "#212121",
		"activityBar.border": "#212121",
		"activityBar.dropBackground": "#212121",
		"activityBar.foreground": "#FFFFFF",
		"activityBar.inactiveForeground": "#CCCCCC",
		"activityBarBadge.background": "#212121",
		"activityBarBadge.foreground": "#212121",

		// Badge - https://code.visualstudio.com/api/references/theme-color#button-control
		"badge.background": "#FAD000",
		"badge.foreground": "#252525",

		//Button - https://code.visualstudio.com/docs/getstarted/theme-color-reference#_button-control
		"button.background": "#252525",
		"button.foreground": "#CCCCCC",
		"button.hoverBackground": "#303030",

		// Base Colors - https://code.visualstudio.com/api/references/theme-color#_base-colors
		"descriptionForeground": "#212121",
		"selection.background": "#FAD000",

		// Dropdowns - https://code.visualstudio.com/docs/getstarted/theme-color-reference#_dropdown-control
		"dropdown.background": "#303030",
		"dropdown.border": "#252525",
		"dropdown.foreground": "#FFFFFF",

		// Editor - https://code.visualstudio.com/docs/getstarted/theme-color-reference
		"editor.background": "#212121",
		"editor.foreground": "#CCCCCC",
		"editorLineNumber.foreground": "#CCCCCC",
		"editorCursor.foreground": "#FAD000",
		"editor.selectionBackground": "#353535",

		// Editor Errors
		"editorError.foreground": "#EC3A37F5",
		"editorWarning.border": "#FFFFFF00",
		"editorWarning.foreground": "#FAD000",

		// Sidebar - https://code.visualstudio.com/docs/getstarted/theme-color-reference#_side-bar
		"sideBar.background": "#212121",
		"sideBar.border": "#303030",
		"sideBar.foreground": "#CCCCCC",
		"sideBarSectionHeader.background": "#252525",
		"sideBarSectionHeader.foreground": "#CCCCCC",
		"sideBarTitle.foreground": "#CCCCCC",
		"sideBarSectionHeader.border": "#252525",

		// Sidebar List - https://code.visualstudio.com/docs/getstarted/theme-color-reference#_lists-and-trees
		"list.dropBackground": "#303030",
		"list.activeSelectionBackground": "#303030",
		"list.activeSelectionForeground": "#CCCCCC",
		"list.focusBackground": "#353535",
		"list.focusForeground": "#CCCCCC",
		"list.highlightForeground": "#FAD000",
		"list.hoverBackground": "#353535",
		"list.hoverForeground": "#CCCCCC",
		"list.inactiveSelectionBackground": "#303030",
		"list.inactiveSelectionForeground": "#AAA",

		// Status Bar - https://code.visualstudio.com/docs/getstarted/theme-color-reference#_status-bar-colors
		"statusBar.border": "#353535",
		"statusBar.background": "#303030",
		"statusBar.foreground": "#CCCCCC",
		"statusBar.debuggingBackground": "#FAD000",
		"statusBar.debuggingForeground": "#303030",
		"statusBar.noFolderBackground": "#303030",
		"statusBar.noFolderForeground": "#CCCCCC",
		"statusBarItem.activeBackground": "#212121",
		"statusBarItem.hoverBackground": "#212121",
		"statusBarItem.prominentBackground": "#303030",
		"statusBarItem.prominentHoverBackground": "#353535",

		// Tabs - https://code.visualstudio.com/api/references/theme-color#editor-groups-tabs
		"tab.activeBackground": "#212121",
		"tab.activeForeground": "#CCCCCC",
		"tab.border": "#212121",
		"tab.activeBorder": "#FAD000",
		"tab.inactiveBackground": "#353535",
		"tab.inactiveForeground": "#CCCCCC",
		"tab.unfocusedActiveForeground": "#353535",
		"tab.unfocusedInactiveForeground": "#CCCCCC",

		// Terminal
		"terminal.ansiBlack": "#000000",
		"terminal.ansiRed": "#EC3A37F5",
		"terminal.ansiGreen": "#3AD900",
		"terminal.ansiYellow": "#FAD000",
		"terminal.ansiBlue": "#202020",
		"terminal.ansiMagenta": "#FF2C70",
		"terminal.ansiCyan": "#80FCFF",
		"terminal.ansiWhite": "#FFFFFF",
		"terminal.ansiBrightBlack": "#5C5C61",
		"terminal.ansiBrightRed": "#EC3A37F5",
		"terminal.ansiBrightGreen": "#3AD900",
		"terminal.ansiBrightYellow": "#FAD000",
		"terminal.ansiBrightBlue": "#202020",
		"terminal.ansiBrightMagenta": "#FB94FF",
		"terminal.ansiBrightCyan": "#80FCFF",
		"terminal.ansiBrightWhite": "#CCCCCC",
		"terminal.background": "#202020",
		"terminal.foreground": "#FFFFFF",
		"terminalCursor.background": "#FAD000",
		"terminalCursor.foreground": "#FAD000",

		// Text & Welcome Page
		"textBlockQuote.background": "#303030",
		"textBlockQuote.border": "#353535",
		"textCodeBlock.background": "#303030",
		"textLink.activeForeground": "#FAD000",
		"textLink.foreground": "#FAD000",
		"textPreformat.foreground": "#FAD000",
		"textSeparator.foreground": "#303030",
		"welcomePage.buttonBackground": "#252525",
		"welcomePage.buttonHoverBackground": "#303030",

		// Title Bar
		"titleBar.activeBackground": "#303030",
		"titleBar.activeForeground": "#FFFFFF",
		"titleBar.inactiveBackground": "#CCCCCC",
		"titleBar.inactiveForeground": "#353535",
		"walkThrough.embeddedEditorBackground": "#303030",
		"widget.shadow": "#353535"
